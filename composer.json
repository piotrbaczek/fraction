{
  "name": "pbaczek/fraction",
  "description": "Library that handles fractions as nominator/denominator",
  "license": "MIT",
  "require": {
    "php": "^7.1 || 8.*",
    "ext-gmp": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.2.7"
  },
  "autoload": {
    "psr-4": {
      "pbaczek\\fraction\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "pbaczek\\fraction\\tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}